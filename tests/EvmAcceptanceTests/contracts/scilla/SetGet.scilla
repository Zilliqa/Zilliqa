scilla_version 0
(***************************************************)
(*               Basic setter and getter           *)
(***************************************************)
contract SetGet
()
field value: Uint128 = Uint128 0

transition Set(v : Uint128)
  value := v
end

transition Emit()
  v <- value;
  ev = {_eventname: "Emit"; sender: _sender; value: v};
  event ev
end
